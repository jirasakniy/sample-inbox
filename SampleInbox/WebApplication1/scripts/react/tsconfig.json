{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "target": "es5",
      "lib": ["es5", "es2015", "dom", "scripthost"]
    },
    "include": [
      "./src/**/*"
    ]
  }